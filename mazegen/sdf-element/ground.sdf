<!-- FLOOR MODEL -->
<model name='ground_plane'>
    <static>1</static>
    <link name='link'>
    <collision name='collision'>
        <geometry>
        <plane>
            <normal>0 0 1</normal>
            <size>100 100</size>
        </plane>
        </geometry>
        <surface>
        <friction>
            <ode>
            <mu>100</mu>
            <mu2>50</mu2>
            </ode>
            <torsional>
            <ode/>
            </torsional>
        </friction>
        <contact>
            <ode/>
        </contact>
        <bounce/>
        </surface>
        <max_contacts>10</max_contacts>
    </collision>
    <visual name='visual'>
        <cast_shadows>0</cast_shadows>
        <geometry>
        <plane>
            <normal>0 0 1</normal>
            <size>100 100</size>
        </plane>
        </geometry>
        <material>
        <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
        </script>
        </material>
    </visual>
    </link>
</model>